---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <!-- Startsidan -->
  <url>
    <loc>{{ site.url }}{{ site.baseurl }}/</loc>
    <changefreq>weekly</changefreq>
    <priority>1.0</priority>
  </url>

  <!-- Ev. genererade sidor under site/content/ -->
  {% for page in site.pages %}
    {% assign p = page.path | downcase %}
    {% if p contains 'site/content/' and page.name != 'index.html' %}
      <url>
        <loc>{{ site.url }}{{ site.baseurl }}{{ page.url | replace:'index.html','' }}</loc>
        <changefreq>monthly</changefreq>
        <priority>0.8</priority>
      </url>
    {% endif %}
  {% endfor %}
</urlset>
