---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <!-- Startsida -->
  <url><loc>{{ site.url }}{{ site.baseurl }}/</loc></url>

  <!-- Alla sidor som innehåller /content/ i sökvägen -->
  {% assign content_pages = site.pages | where_exp: "p", "p.path contains 'content/'" %}
  {% for page in content_pages %}
    {% assign loc = page.url | replace:'index.html','' %}
    <url><loc>{{ site.url }}{{ site.baseurl }}{{ loc }}</loc></url>
  {% endfor %}
</urlset>
